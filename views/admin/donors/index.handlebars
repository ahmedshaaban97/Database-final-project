<table class="table">
    <thead>
    <tr>
        <th scope="col">First Name</th>
        <th scope="col">Last Name</th>
        <th scope="col">SSN</th>
        <th scope="col">Blood Type</th>
        <th scope="col">Weight</th>
        <th scope="col">Age</th>
    </tr>
    </thead>
    <tbody>
    {{#each donors}}
    <tr>
        <td>{{Fname}}</td>
        <td>{{Lname}}</td>
        <td>{{ssn}}</td>
        <td>{{bloodType}}</td>
        <td>{{weight}}</td>
        <td>{{age}}</td>
    </tr>
    {{/each}}

    </tbody>
</table>